<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.sheedon.component">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kitviewmodel">
        <activity
            android:name=".lifecycle.LifecycleActivity"
            android:exported="false" />
        <activity
            android:name=".arouter.OtherActivity"
            android:exported="false" />
        <activity
            android:name=".arouter.SpareActivity"
            android:exported="false" />
        <activity
            android:name=".arouter.TargetActivity"
            android:exported="false" />
        <activity
            android:name=".notification.NotificationActivity"
            android:exported="false" />
        <activity
            android:name=".upgrade.UpgradeActivity"
            android:exported="true" />
        <activity
            android:name=".NextActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>